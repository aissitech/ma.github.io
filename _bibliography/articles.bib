@article{LHCb:2021eaf,
  abbr={JHEP},
  title={Search for the doubly charmed baryon \(\it{\Xi}_{cc}^{+}\) in the \(\it{\Xi}_{c}^{+} \pi^{-} \pi^{+}\) final state},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={A search for the doubly charmed baryon \(\it{\Xi}_{cc}^{+}\) is performed in the \(\it{\Xi}_{c}^{+} \pi^{-} \pi^{+}\) 
            decay mode, where the \(\it{\Xi}_{c}^{+}\) baryon is reconstructed in the \(p K^- \pi^+\) final state. The study uses 
            proton-proton collision data collected with the LHCb detector at a centre-of-mass energy of \(13~\rm{TeV}\), 
            corresponding to a total integrated luminosity of \(5.4~\rm{fb}^{-1}\). No significant signal is observed in the 
            invariant-mass range of \(3.4 - 3.8~\rm{GeV}/c^2\). Upper limits are set on the ratio of branching fractions 
            multiplied by the production cross-section with respect to the \(\it{\Xi}_{cc}^{+} \to \left ( \it{\Xi}_{c}^{+} \to 
            p K^- \pi^+ \right ) \pi^+\) decay for different \(\it{\Xi}_{cc}^{+}\) mass and lifetime hypotheses in the rapidity 
            range from \(2.0\) to \(4.5\) and the transverse momentum range from \(2.5\) to \(25~\rm{GeV}/c\). The results from 
            this search are combined with a previously published search for the \(\it{\Xi}_{cc}^{+} \to \Lambda_c^+ K^- \pi^+\) 
            decay mode, yielding a maximum local significance of \(4.0\) standard deviations around the mass of 
            \(3620~\rm{MeV}/c^2\), including systematic uncertainties. Taking into account the look-elsewhere effect in the 
            \(3.5 - 3.7~\rm{GeV}/c^2\) mass window, the combined global significance is \(2.9\) standard deviations including 
            systematic uncertainties.},
  arxiv={2109.07292},
