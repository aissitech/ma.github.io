  selected={false}
}

@article{LHCb:2021pyk,
  abbr={JHEP},
  title={Measurement of \(J/\psi\) production cross-sections in \(pp\) collisions at \(\sqrt{s}=5~\rm{TeV}\)},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={The production cross-sections of \(J/\psi\) mesons in proton-proton collisions at a 
            centre-of-mass energy of \(\sqrt{s} = 5~\rm{TeV}\) are measured using a data sample 
            corresponding to an integrated luminosity of \(9.13 \pm 0.18~\rm{pb}^{-1}\), collected 
            by the LHCb experiment. The cross-sections are measured differentially as a function of 
            transverse momentum, \(p_T\), and rapidity, \(y\), and separately for \(J/\psi\) mesons 
            produced promptly and from beauty hadron decays (nonprompt). With the assumption of unpolarised 
            \(J/\psi\) mesons, the production cross-sections integrated over the kinematic range 
            \(0 < p_T < 20~\rm{GeV}/c\) and \(2.0 < y < 4.5\) are \(8.154 \pm 0.010 \pm 0.283~\mu\rm{b}\) 
            for prompt \(J/\psi\) mesons and \(0.820 \pm 0.003 \pm 0.034~\mu\rm{b}\) for nonprompt \(J/\psi\)
            mesons, where the first uncertainties are statistical and the second systematic. These cross-sections 
            are compared with those at \(\sqrt{s} = 8~\rm{TeV}\) and \(13~\rm{TeV}\), and are used 
            to update the measurement of the nuclear modification factor in proton-lead collisions for \(J/\psi\) 
            mesons at a centre-of-mass energy per nucleon pair of \(s_{NN} = \sqrt{5}~\rm{TeV}\). The results 
            are compared with theoretical predictions.},
  arxiv={2109.00220},
  journal={JHEP},
  volume={11},
  pages={181},
  doi={10.1007/JHEP11(2021)181},
  url={http://dx.doi.org/10.1007/JHEP11(2021)181},
  data={https://www.hepdata.net/record/ins1915030},
  year={2021},
  month={11},
  day={24},
  selected={false}
}

