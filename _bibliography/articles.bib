@article{LHCb:2022pjv,
  abbr={JHEP},
  title={Amplitude analysis of the \(D_s^+ \to \pi^- \pi^+ \pi^+\) decay},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={A Dalitz plot analysis of the \(D_s^+ \to \pi^- \pi^+ \pi^+\) decay is presented. The analysis is based on 
            proton-proton collision data recorded by the LHCb experiment at a centre-of-mass energy \(\sqrt{s} = 
            8~\rm{TeV}\) and corresponding to an integrated luminosity of \(1.5~\rm{fb}^{-1}\). The resonant structure 
            of the decay is obtained using a quasi-model-independent partial-wave analysis, in which the \(\pi^+ \pi^-\)
            S-wave amplitude is parameterised as a generic complex function determined by a fit to the data. The 
            S-wave component is found to be dominant, followed by the contribution from spin-2 resonances and a small
            contribution from spin-1 resonances. The latter includes the first observation of the 
            \(D_s^+ \to \omega(782) \pi^+\) channel in the \(D_s^+ \to \pi^- \pi^+ \pi^+\) decay. The resonant structures
            of the \(D_s^+ \to \pi^- \pi^+ \pi^+\) and \(D^+ \to \pi^- \pi^+ \pi^+\) decays are compared, providing 
            information about the mechanisms for the hadron formation in these decays.},
  arxiv={2209.09840},
  journal={JHEP},
  volume={07},
  pages={204},
  doi={10.1007/JHEP07(2023)204},
  url={https://doi.org/10.1007/JHEP07(2023)204},
  cds={https://cds.cern.ch/record/2834485},
  year={2023},
  month={07},
  day={26},
  selected={false}
}

@article{LHCb:2022tyw,
  abbr={EPJC},
  title={\(J/\psi\) and \(D^0\) production in \(\sqrt{s_{\rm{NN}}} = 68.5~\rm{GeV}\) \(\rm{PbNe}\) collisions},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={The first measurement of \(J/\psi\) and \(D^0\) production in \(\rm{PbNe}\) collisions by the LHCb experiment 
            in its fixed-target configuration is reported. The production of \(J/\psi\) and \(D^0\) mesons is studied with 
            a beam of lead ions with an energy of \(2.5~\rm{TeV}\) per nucleon colliding on gaseous neon targets at rest, 
            corresponding to a nucleon-nucleon centre-of-mass energy of \(\sqrt{s_{\rm{NN}}} = 68.5~\rm{GeV}\). The 
            \(J/\psi D^0\) production cross-section ratio is studied as a function of rapidity, transverse momentum and 
            collision centrality. These data are compared with measurements from \(p\rm{Ne}\) collisions at the same energy. 
            No anomalous \(J/\psi\) suppression that could indicate the formation of a deconfined medium is observed.},
  arxiv={2211.11652},
  journal={Eur. Phys. J. C},
  volume={83},
  pages={658},
