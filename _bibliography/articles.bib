}

@article{LHCb:2021stx,
  abbr={PRL},
  title={Study of \(Z\) bosons produced in association with charm in the forward region},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={The fraction of \(Z\)+jet events that contain a charm jet is measured for the first time in the forward region of 
            proton-proton collisions. The data sample used corresponds to an integrated luminosity of \(6~\rm{fb}^{-1}\)
            collected at a center-of-mass energy of \(13~\rm{TeV}\) with the LHCb detector. The ratio of production cross 
