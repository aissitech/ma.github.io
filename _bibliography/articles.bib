@article{LHCb:2021auc,
  abbr={Nat. Commun.},
  title={Study of the doubly charmed tetraquark \(T_{cc}^+\)},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={An exotic narrow state in the \(D^0 D^0 \pi^+\) mass spectrum just below the \(D^{*+} D^0\) mass threshold is studied 
            using a data set corresponding to an integrated luminosity of \(9~\rm{fb}^{-1}\) acquired with the LHCb detector in 
            proton-proton collisions at centre-of-mass energies of \(7\), \(8\) and \(13~\rm{TeV}\). The state is consistent with 
            the ground isoscalar \(T_{cc}^+\) tetraquark with a quark content of \(c c \bar{u} \bar{d}\) and spin-parity quantum 
            numbers \(J^P = 1^+\). Study of the \(DD\) mass spectra disfavours interpretation of the resonance as the isovector 
            state. The decay structure via intermediate off-shell \(D^{*+}\) mesons is confirmed by the \(D^0 \pi^+\) mass 
            distribution. The mass of the resonance and its coupling to the \(D^* D\) system are analysed. Resonance parameters 
            including the pole position, scattering length, effective range and compositeness are measured to reveal important 
            information about the nature of the \(T_{cc}^+\) state. In addition, an unexpected dependence of the production rate 
            on track multiplicity is observed.},
  arxiv={2109.01056},
  journal={Nat. Commun.},
  volume={13},
  pages={3351},
  issue={1},
  doi={10.1038/s41467-022-30206-w},
  url={https://doi.org/10.1038/s41467-022-30206-w},
  data={https://www.hepdata.net/record/ins1915358},
  year={2022},
  month={06},
  day={16},
  selected={true}
}

@article{LHCb:2021vvq,
  abbr={Nat. Phys.},
  title={Observation of an exotic narrow doubly charmed tetraquark},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={Conventional hadronic matter consists of baryons and mesons made of three quarks and quark-antiquark pairs, 
            respectively. The observation of a new type of hadronic state, a doubly charmed tetraquark containing two charm 
            quarks, an anti-\(u\) and an anti-\(d\) quark, is reported using data collected by the LHCb experiment at the 
            Large Hadron Collider. This exotic state with a mass of about \(3875~\rm{MeV}/c^2\) manifests itself as a narrow 
            peak in the mass spectrum of \(D^0 D^0 \pi^+\) mesons just below the \(D^{*+} D^0\) mass threshold. The near 
            threshold mass together with a strikingly narrow width reveals the resonance nature of the state.},
  arxiv={2109.01038},
  journal={Nat. Phys.},
  volume={},
  pages={},
  issue={},
  doi={10.1038/s41567-022-01614-y},
  url={https://doi.org/10.1038/s41567-022-01614-y},
  data={https://www.hepdata.net/record/ins1915457},
  year={2022},
  month={06},
  day={16},
  selected={true}
}

@article{LHCb:2021yxk,
  abbr={PRL},
  title={Angular analysis of \(D^0 \to \pi^+\pi^-\mu^+\mu^-\) and \(D^0 \to K^+K^-\mu^+\mu^-\) decays and search for \(CP\) 
         violation},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={The first full angular analysis and an updated measurement of the decay-rate \(CP\) asymmetry of the \(D^0 \to 
            \pi^+\pi^-\mu^+\mu^-\) and \(D^0 \to K^+K^-\mu^+\mu^-\) decays are reported. The analysis uses proton-proton 
            collision data collected with the LHCb detector at centre-of-mass energies of \(7\), \(8\) and \(13~\rm{TeV}\). 
            The data set corresponds to an integrated luminosity of \(9~\rm{fb}^{-1}\). The full set of \(CP\)-averaged angular
            observables and their \(CP\) asymmetries are measured as a function of the dimuon invariant mass. The results are 
            consistent with expectations from the Standard Model and with \(CP\) symmetry.},
  arxiv={2111.03327},
  journal={Phys. Rev. Lett.},
  volume={128},
  pages={221801},
  issue={22},
  doi={10.1103/PhysRevLett.128.191803},
  url={https://link.aps.org/doi/10.1103/PhysRevLett.128.221801},
  year={2022},
  month={6},
  day={3},
  selected={false}
}

@article{LHCb:2022gnc,
  abbr={PRD},
  title={Measurement of the charm mixing parameter \(y_{CP} - y_{CP}^{K\pi}\) using two-body \(D^0\) meson decays},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={A measurement of the ratios of the effective decay widths of \(D^0 \to \pi^- \pi^+\) and \(D^0 \to K^- K^+\) decays 
            over that of \(D^0 \to K^- \pi^+\) decays is performed with the LHCb experiment using proton-proton collisions at a 
            centre-of-mass energy of \(13~\rm{TeV}\), corresponding to an integrated luminosity of \(6~\rm{fb}^{-1}\). These 
            observables give access to the charm mixing parameters \(y_{CP}^{\pi\pi} - y_{CP}^{K\pi}\) and \(y_{CP}^{KK} - 
            y_{CP}^{K\pi}\), and are measured as \(y_{CP}^{\pi\pi} - y_{CP}^{K\pi} = ( 6.57 \pm 0.53 \pm 0.16 ) \times 10^{-3}\), 
            \(y_{CP}^{KK} - y_{CP}^{K\pi} = ( 7.08 \pm 0.30 \pm 0.14 ) \times 10^{-3}\), where the first uncertainties are 
            statistical and the second systematic. The combination of the two measurements is \(y_{CP} - y_{CP}^{K\pi} = ( 6.96
            \pm 0.26 \pm 0.13 ) \times 10^{-3}\), which is four times more precise than the previous world average.},
  arxiv={2202.09106},
  journal={Phys. Rev. D},
  volume={105},
  issue={9},
  pages={092013},
  doi={10.1103/PhysRevD.105.092013},
  url={https://link.aps.org/doi/10.1103/PhysRevD.105.092013},
  year={2022},
  month={05},
  day={25},
  selected={false}
}

@article{LHCb:2022piu,
  abbr={PRL},
  title={Observation of the decay \(\Lambda_b^0 \to \Lambda_c^+ \tau^- \overline{\nu}_{\tau}\)},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={The first observation of the semileptonic \(b\)-baryon decay \(\Lambda_b^0 \to \Lambda_c^+ \tau^- 
            \overline{\nu}_{\tau}\), with a significance of \(6.1 \sigma\), is reported using a data sample corresponding to 
            \(3~\rm{fb}^{-1}\) of integrated luminosity, collected by the LHCb experiment at centre-of-mass energies of \(7\) 
            and \(8~\rm{TeV}\) at the LHC. The \(\tau^-\) lepton is reconstructed in the hadronic decay to three charged pions. 
            The branching fraction \(\mathcal{B} (\Lambda_b^0 \to \Lambda_c^+ \tau^- \overline{\nu}_{\tau}) = (1.50 \pm 0.16 
            \pm 0.25 \pm 0.23) \%\) is obtained, where uncertainties are statistical, systematic and from the external branching 
            fraction of the normalisation channel \(\Lambda_b^0 \to \Lambda_c^+ \pi^- \pi^+ \pi^-\). The ratio of semileptonic 
            branching fractions \(\mathcal{R} (\Lambda_c^+) \equiv \mathcal{B} (\Lambda_b^0 \to \Lambda_c^+ \tau^- 
            \overline{\nu}_{\tau}) / \mathcal{B} (\Lambda_b^0 \to \Lambda_c^+ \mu^- \overline{\nu}_{\mu})\) is derived to be 
            \(0.242 \pm 0.026 \pm 0.040 \pm 0.059\), where the external branching fraction uncertainty from the channel 
            \(\Lambda_b^0 \to \Lambda_c^+ \mu^- \overline{\nu}_{\mu}\) contributes to the last term. This result is in agreement 
            with the Standard Model prediction.},
  arxiv={2201.03497},
  journal={Phys. Rev. Lett.},
  volume={128},
  pages={191803},
  issue={19},
  doi={10.1103/PhysRevLett.128.191803},
  url={https://link.aps.org/doi/10.1103/PhysRevLett.128.191803},
  year={2022},
  month={05},
  day={13},
  selected={false}
}

@article{LHCb:2021lvy,
  abbr={PRL},
  title={Tests of lepton universality using \(B^0\to K^0_S \ell^+ \ell^-\) and \(B^+\to K^{*+}\ell^+\ell^-\) decays},
  collaboration={LHCb Collaboration},
  author={Aaij, R. and others},
  abstract={Tests of lepton universality in \(B^0\to K^0_S \ell^+ \ell^-\) and \(B^+\to K^{*+}\ell^+\ell^-\) decays where \(\ell\) is 
            either an electron or a muon are presented. The differential branching fractions of \(B^0\to K^0_S e^+ e^-\) and 
            \(B^+\to K^{*+} e^+ e^-\) decays are measured in intervals of the dilepton invariant mass squared. The measurements 
            are performed using proton-proton collision data recorded by the LHCb experiment, corresponding to an integrated 
            luminosity of \(9~\rm{fb}^{-1}\). The results are consistent with the Standard Model and previous tests of lepton 
            universality in related decay modes. The first observation of \(B^0\to K^0_S e^+ e^-\) and \(B^+\to K^{*+} e^+ e^-\) 
            decays is reported.},
